<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.VBox?>

<VBox xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.waymaker.Main">
    <children>
        <ImageView fx:id="imageView" fitHeight="400.0" fitWidth="600.0"/>
        <Button text="Upload Image" onAction="#handleUpload"/>
        <Label fx:id="mbtiLabel" text="MBTI Type: "/>
        <Label fx:id="rankLabel" text="Rank: "/>
    </children>
    <padding>
        <Insets top="20" right="20" bottom="20" left="20"/>
    </padding>
</VBox>
